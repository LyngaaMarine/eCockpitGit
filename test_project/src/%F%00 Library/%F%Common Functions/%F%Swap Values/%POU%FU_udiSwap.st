FUNCTION FU_udiSwap : BOOL
VAR_IN_OUT
	uiA		:UDINT;		//Value a to be swapped with b
	uiB		:UDINT;		//Value b to be swapped with a 
END_VAR
VAR
	C		:UDINT;		//Holding variable
END_VAR
(*%!%__DELIMITER__%!%*)
C		:=		uiA;
uiA		:=		uiB;
uiB		:=		C;